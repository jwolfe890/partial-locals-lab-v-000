<%= form_tag students_path, method: "get" do %>
<%= label_tag :name %>
<%= text_field_tag :name, params[:name] %>
<%= submit_tag "Search" %>
<% end %>

<% if @student_names.present? %>

<% @student_names.each do |student| %>

<%= render partial: 'student', locals: {student: student} %> 

<% end %>

<% end %>